n=int(input("Enter the number"))
if(n==1):
    print("Sunday")
elif(n==2):
    print("Monday")
elif(n==3):
    print("Tuesday")
elif(n==4):
    print("wednesday")    
elif(n==5):
    print("thursday")    
elif(n==6):
    print("friday")
elif(n==7):
    print("saturday")
else:
    print("Error")
    
